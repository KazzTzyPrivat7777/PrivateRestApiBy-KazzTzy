{
  "name": "KayzzAoshi?",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/src dist/router && cp -r src/*.json src/*.jpg dist/src/ 2>/dev/null || true && cp -r public dist/ 2>/dev/null || true",
    "start": "node dist/index.js",
    "dev": "ts-node index.ts",
    "dev:watch": "nodemon --exec ts-node index.ts",
    "pm2": "pm2 start index.ts --name kuroneko --interpreter node --node-args=\"-r ts-node/register\" && pm2 save"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.69",
    "axios": "^1.6.0",
    "axios-cookiejar-support": "^5.0.0",
    "canvas": "^3.2.1",
    "cheerio": "^1.0.0",
    "@putuofc/assetsku": "*",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "canvafy": "^7.0.4",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "https": "^1.0.0",
    "multer": "^1.4.5-lts.1",
    "jimp": "^0.22.10",
    "node-fetch": "^2.7.0",
    "sharp": "^0.33.2",
    "tough-cookie": "^4.1.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "uuid": "^9.0.1",
    "yt-search": "^2.12.1",
    "buffer": "^6.0.3",
    "os": "^0.1.2",
    "undici": "^6.6.2",
    "puppeteer": "^22.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.5.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^9.0.7",
    "@types/yt-search": "^2.10.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "nodemon": "^3.0.2"
  }
}